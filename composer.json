{
    "name": "stubbles/sequence",
    "type": "library",
    "homepage": "http://stubbles.net/",
    "license": "BSD-3-Clause",
    "description": "Stream your iterators.",
    "require": {
        "php": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.4",
        "bovigo/assert": "^2.0",
        "bovigo/callmap": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "stubbles\\sequence\\": "src/main/php"
        },
        "files": ["src/main/php/functions.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "stubbles\\test\\": "src/test/helper"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "8.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
