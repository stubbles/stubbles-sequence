{
    "name": "stubbles/sequence",
    "type": "library",
    "homepage": "http://stubbles.net/",
    "license": "BSD-3-Clause",
    "description": "Stream your iterators.",
    "require": {
        "php": "^7.3",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "bovigo/assert": "^6.0",
        "bovigo/callmap": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "stubbles\\sequence\\": "src/main/php"
        },
        "files": ["src/main/php/functions.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "stubbles\\test\\": "src/test/helper",
            "stubbles\\sequence\\": "src/test/php"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "9.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "phpunit",
        "php-coveralls": "php-coveralls"
    }
}
